{
    "debug": true,
    "hostname": {{ default .Env.HOSTNAME "\"\"" }},
    "ip": {{ default .Env.IP "\"\"" }},
    "cadvisorPort": {{ default .Env.CADVISORPORT "\"4194\""}},
    "apiserver": {{ default .Env.APISERVER "\"\""}},
    "plugin": {
        "enabled": {{ default .Env.PLUGIN_ENABLED "false" }},
        "dir": {{ default .Env.PLUGIN_DIR "\"./plugin\"" }},
        "git": {{ default .Env.PLUGIN_GIT "\"https://github.com/open-falcon/plugin.git\"" }},
        "logs": {{ default .Env.PLUGIN_LOGS "\"./logs\"" }}
    },
    "heartbeat": {
        "enabled": {{ default .Env.HEARTBEAT_ENABLED "false" }},
        "addr": {{ default .Env.HEARTBEAT_ADDR "\"127.0.0.1:6030\"" }},
        "interval": {{ default .Env.HEARTBEAT_INTERVAL "60" }},
        "timeout": {{ default .Env.HEARTBEAT_TIMEOUT "1000" }}
    },
    "transfer": {
        "enabled": {{ default .Env.TRANSFER_ENABLED "true" }},
        "addrs": {{ default .Env.TRANSFER_ADDR "[\"127.0.0.1:8433\",\"127.0.0.1:8433\"]" }},
        "interval": {{ default .Env.TRANSFER_INTERVAL "60" }},
        "timeout": {{ default .Env.TRANSFER_TIMEOUT "1000" }}
    },
    "http": {
        "enabled": {{ default .Env.HTTP_ENABLED "true" }},
        "listen": {{ default .Env.HTTP_LISTEN "\":1988\"" }},
        "backdoor": {{ default .Env.HTTP_BACKDOOR "false" }}
    },
    "collector": {
        "ifacePrefix": {{ default .Env.COLLECTOR "[\"eth\", \"em\"]" }}
    },
